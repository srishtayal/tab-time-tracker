{
    "manifest_version": 3,
    "name": "Tab Time Tracker",
    "version": "1.0",
    "description": "Track the time you spend on each tab",
    "permissions": ["tabs", "storage"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    }
}
